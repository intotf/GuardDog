<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--删除文件的目录-->
    <add key ="Dir" value="temp"/>
    <!-- 轮训间隔时间,单位分钟 1440分钟 = 24小时 -->
    <add key="IntervalTime" value="1"/>
    <!--删除的文件通配-->
    <add key="Filter" value=".jpg|.jpge"/>
    <!--删除在Days天以前创建的文件-->
    <add key ="Days" value="7"/>
    <!--是否删除-->
    <add key="Delete" value="false"/>
    <!--是否删除空文件夹-->
    <add key="DeleteEmptyDir" value="true"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
